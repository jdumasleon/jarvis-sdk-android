    <!-- Suppress other common pre-existing warnings -->
    <issue id="ContentDescription" severity="warning" />
    <issue id="UsingOnClickInXml" severity="warning" />
    <issue id="TextFields" severity="warning" />
    <issue id="ButtonCase" severity="warning" />
